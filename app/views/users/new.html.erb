<h1>Sign up</h1>

    <%= form_for(@user) do |f| %>
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>
      
      <%= f.label :last_name %>
      <%= f.text_field :last_name %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      
      <%= f.label :street_name %>
      <%= f.text_field :street_name %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>


